<?xml version='1.0' encoding='UTF-8'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="3.0">
  <display-name>HelloWorldServiceWebApp</display-name>
  <servlet>
    <servlet-name>HelloWorldServiceServlethttp</servlet-name>
    <servlet-class>com.oracle.csm.soapwstemplate.HelloWorldImpl</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloWorldServiceServlethttp</servlet-name>
    <url-pattern>/HelloWorldService</url-pattern>
  </servlet-mapping>
</web-app>
